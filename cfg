//Trin's Pyro Panic Script
unbind "/"
alias "battlecry" "voicemenu 2 1";
alias "+panic" "battlecry; cl_yawspeed 3000; +left; +attack";
alias "-panic" "-attack; -left; cl_yawspeed 210";
bind "/" "+panic";
//end of Pyro Panic Script

//Trin's Zoomscript
alias "+zoom" "zoomin"
alias "-zoom" "zoomout"
alias "zoomin" "r_drawviewmodel 0; fov_desired 75"
alias "zoomout" "r_drawviewmodel 1; fov_desired 90"
unbind MOUSE5
bind "MOUSE5" "+zoom"
//end of Zoomscript

//more than a scoreboard by Trin
unbind tab
alias +info "+showscores; net_graph 3; cl_showfps 2"
alias -info "-showscores; net_graph 0; cl_showfps 1"
bind tab +info

//high five toggle
unbind H
alias enable_action_item "alias toggle_action_item disable_action_item; +use_action_slot_item"
alias disable_action_item "alias toggle_action_item enable_action_item; -use_action_slot_item" 
alias toggle_action_item enable_action_item
bind H toggle_action_item

